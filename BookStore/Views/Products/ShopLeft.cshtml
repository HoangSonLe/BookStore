@model IEnumerable<Publishers>
<div class="col-lg-3 col-md-3 col-sm-4 col-xs-12">
    <div class="shop-left">
        <div class="section-title-5 mb-30">
            <h2>Lựa chọn khác</h2>
        </div>
        <div class="left-title mb-20">
            <h4>Nhà xuất bản</h4>
        </div>
        <div class="left-menu mb-30">
            <ul>
                @foreach (var p in Model)
                {
                    <li><a  asp-action="SelectByPublisher" asp-controller="Products" asp-route-urlfriendly="@p.UrlFrienfly">@p.PublisherName</a></li>
                }
            </ul>
        </div>
    </div>
</div>

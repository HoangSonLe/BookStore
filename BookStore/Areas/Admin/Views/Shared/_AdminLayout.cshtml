<!DOCTYPE html>
<html>
<!-- Mirrored from shamsoft.net/flaty/ by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 25 Sep 2015 07:23:28 GMT -->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>@ViewBag.Title</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <!--base css styles-->
    <link rel="stylesheet" href="~/Admin/assets/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Admin/assets/font-awesome/css/font-awesome.min.css">
    <!--page specific css styles-->
    <!--flaty css styles-->
    <link rel="stylesheet" href="~/Admin/css/flaty-responsive.css">
    <link href="~/Admin/css/flaty1.css" rel="stylesheet" />
</head>

<body>
    @await Html.PartialAsync("_MyHeader.cshtml")
    <!-- BEGIN Container -->
    <div class="container" id="main-container">
        @await Html.PartialAsync("_MyMenu.cshtml")

        <!-- BEGIN Content -->
        <div id="main-content">
            <!-- BEGIN Page Title -->
            <div class="page-title">
                <div>
                    <h1><i class="fa fa-file-o"></i> @ViewBag.Title</h1>
                </div>
            </div>
            <!-- END Page Title -->
            <!-- BEGIN Breadcrumb -->
            <div id="breadcrumbs">
                <ul class="breadcrumb">
                    <li class="active"><i class="fa fa-home"></i>Home/@ViewBag.Title</li>
                </ul>
            </div>
            <!-- END Breadcrumb -->
            @RenderBody()
            <footer>
                <p>2013 © FLATY Admin Template.</p>
            </footer>
            <a id="btn-scrollup" class="btn btn-circle btn-lg" href="#"><i class="fa fa-chevron-up"></i></a>
        </div>
        <!-- END Content -->
    </div>
    <!-- END Container -->
    <!-- BEGIN Container -->
    <!-- END Container -->
    <!--basic scripts-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="~/Admin/assets/jquery/jquery-2.1.1.min.js"><\/script>')</script>
    <script src="~/Admin/assets/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Admin/assets/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <script src="~/Admin/assets/jquery-cookie/jquery.cookie.js"></script>
    <!--page specific plugin scripts-->
    <script src="~/Admin/assets/flot/jquery.flot.js"></script>
    <script src="~/Admin/assets/flot/jquery.flot.resize.js"></script>
    <script src="~/Admin/assets/flot/jquery.flot.pie.js"></script>
    <script src="~/Admin/assets/flot/jquery.flot.stack.js"></script>
    <script src="~/Admin/assets/flot/jquery.flot.crosshair.js"></script>
    <script src="~/Admin/assets/flot/jquery.flot.tooltip.min.js"></script>
    <script src="~/Admin/assets/sparkline/jquery.sparkline.min.js"></script>
    <!--flaty scripts-->
    <script src="~/Admin/js/flaty.js"></script>
    <script src="~/Admin/js/flaty-demo-codes.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
    @RenderSection("Scripts", required: false)
</body>
<!-- Mirrored from shamsoft.net/flaty/ by HTTrack Website Copier/3.x [XR&CO'2014], Fri, 25 Sep 2015 07:24:12 GMT -->
</html>
